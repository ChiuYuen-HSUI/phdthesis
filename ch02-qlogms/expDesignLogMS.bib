@INPROCEEDINGS{GeerardynIMTC2012,
  author = {Egon Geerardyn and Yves Rolain and Johan Schoukens},
  title = {Quasi-logarithmic Multisine Excitations for Broad Frequency Band
	Measurements},
  booktitle = {I2MTC 2012, IEEE International Instrumentation and Measurement Technology
	Conference, May 13-16},
  year = {2012},
  pages = {737--741},
  address = {Graz, Austria},
  month = {May},
  abstract = {Logarithmically distributed excitation signals are widely used to
	measure the transfer function of dynamic systems in a wide frequency
	band, covering several decades. Periodic signals are very popular
	in advanced dynamic signal analyzers. Generating periodic signals
	requires an equidistant frequency grid which conflicts with the logarithmic
	distribution, especially at the low frequencies. In this paper we
	offer an elegant solution to get around this problem that also makes
	an improved choice for the amplitude spectrum.},
  owner = {egon},
  timestamp = {2012.05.31}
}


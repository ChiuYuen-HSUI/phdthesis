@Article{Abbas2012,
  Title                    = {{System Identification in {GSM/EDGE} Receivers Using a Multi-Model Approach}},
  Author                   = {Abbas, Taimoor and Tufvesson, },
  Journal                  = {{ACEEE} International Journal on Control System and Instrumentation},
  Year                     = {2012},
  Number                   = {01},
  Pages                    = {41--46},
  Volume                   = {03},

  Abstract                 = {Model order selection is an important element in system identification. It is well known that common model order selection methods such as Akaike's information criterion (AIC) and Bayesian's information criterion (BIC) neglect relevant information that is available in models of order different from the one chosen. In this paper the model order selection problem for receivers similar to those found in GSM and EDGE systems is reviewed briefly and is solved with a multi-model approach based on simultaneous consideration of several models. Two methods are evaluated; a multi-model noise suppression filter and multi-model soft combining. The algorithms are implemented and evaluated by means of simulations. The performance of each method is analyzed for GSM and EDGE receivers in a link level simulator. Simulation results show a significant improvement in performance at the cost of increased computational complexity for the multi-model approach. Index Terms-System Identification, Multi-model, GSM, EDGE, Whitening, Equalizer},
  Doi                      = {01.IJCSI.03.01.36},
  File                     = {Abbas2012 - System Identification in GSM-EDGE Receivers Using a Multi-Model Approach.pdf:Abbas2012 - System Identification in GSM-EDGE Receivers Using a Multi-Model Approach.pdf:PDF},
  Owner                    = {egon.geerardyn},
  Timestamp                = {2013.04.09}
}

